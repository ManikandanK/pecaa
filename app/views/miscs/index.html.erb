<h1>Listing miscs</h1>

<table>
  <tr>
    <th>User</th>
    <th>Title</th>
    <th>Address</th>
    <th>Is deleted</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @miscs.each do |misc| %>
  <tr>
    <td><%= misc.user_id %></td>
    <td><%= misc.title %></td>
    <td><%= misc.address %></td>
    <td><%= misc.is_deleted %></td>
    <td><%= link_to 'Show', misc %></td>
    <td><%= link_to 'Edit', edit_misc_path(misc) %></td>
    <td><%= link_to 'Destroy', misc, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Misc', new_misc_path %>
