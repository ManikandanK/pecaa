                          <%= hidden_field(:site_payment_gateway, :is_enable, :value => site_payment_gateway_test_mode_arr[2]) %>
                        </span>
                          </span>
                            <div class="colum-section">
                              <div class="group"> <span class="key">name</span> <span class="val">
                                <%= shipping_gateway_attribute.text_field(:name, :class => {:size => 20}, :value => 'Ground')%>
                                </span> </div>
                              <div class="group"> <span class="key">display name:</span> <span class="val">
                                <%= shipping_gateway_attribute.text_field(:display_name, :class => {:size => 20}, :value => @site_payment_gateway.get_value_of("display_name"))%>
                                </span> </div>
                              <div class="group next"> <span class="key">price modifier:</span> <span class="val">
                                <select class="noEdit">
                                  <option>$</option>
                                </select>
                                </span>
                               </div>
                                <div class="group"> <span class="key"></span> <span class="val">
                               <img width="61" height="24" src="css/img/btn_delete.jpg">
                                </span> </div>

                              </div>
                            </div>
                            <div class="userGrid cartGrid">
                            <div class="userGridHeader noRadius">
