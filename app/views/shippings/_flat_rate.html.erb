          <div class="content-section categoryMain">
            <div class="pages-main noHeight">
              <div class="pages-wrapper">
                <div class="site-users left nobMargin">
                  <div class="rowDiv2">
                    <div class="pages-wrapper">
                     <div class="usersLeftPane productsTitle">
                           <div class="title">flat rate options</div>
                         </div>
                         <div class="rightButtonDiv">
                           <input type="button" value="create new flat rate method" class="addWeightBtn" name="input">
                           <span class="searchIcon"><a href="#"></a></span> </div>
                      <!-- grid-->
                      <%= form_for(@payment_gateway.shipping_gateway_attributes, :url => site_shipping_path(@site.id, params[:id]), :method => :put) do |shipping_gateway_attribute| %>
                      <div class="site-users nobMargin ">
                        <div class="page-data clear">
                          <div class="manageDateLeftBox"> <span>
                            <span class="status">
                          <% site_payment_gateway_test_mode_arr = site_payment_gateway_test_mode %>
