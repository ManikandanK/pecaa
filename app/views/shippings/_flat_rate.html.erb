                                      <li class="alt"> <span class="cName arrow2"></span><span class="cName arrow2">$</span><span class="cName arrow2">$10.00</span><span class="close1"></span> </li>

                                    </ul>
                                  </div>
                                </div>
                              </div>
                            </div>
                            <div class="userGridFooter"></div>
                          </div>
                          </div>

                        </div>
                      <% end %>
                      </div>
                    </div>
                  </div>
