	<span class="key">Name:</span>
	<span class="val"><%=contact_fields.text_field :name %></span>
</div>
<div class="group">
	<span class="key">position:</span>
	<span class="val"><%=contact_fields.text_field :position %></span>
</div>
<div class="group">
	<span class="key">email:</span>
	<span class="val"><%=contact_fields.text_field :email %></span>
</div>
<div class="group">
	<span class="key">phone:</span>
	<span class="val"><%=contact_fields.text_field :phone %></span>
</div>
<%end%>
