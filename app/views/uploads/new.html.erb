<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
  <head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>Upload File</title>
    <link href="css/pecaa.css" rel="stylesheet" type="text/css" />
    <!-- tabs -->
    <link href="tabs/tabs.css" rel="stylesheet" type="text/css" />
    <!-- end of tabs script -->

    <!-- select style scripts -->
    <link href="style-select/select.css" rel="stylesheet" type="text/css" />
    <script type="text/javascript" src="style-select/jquery.js"></script>
    <script type="text/javascript" src="style-select/customSelect.js"></script>
    <!-- end of select style scripts -->

    <!-- slider scripts -->
    <link type="text/css" href="slider/jquery-ui-1.8.2.custom.css" rel="stylesheet" />
    <script type="text/javascript" src="slider/jquery-1.4.2.min.js"></script>
    <script type="text/javascript" src="slider/jquery-ui-1.8.2.custom.min.js"></script>
    <script type="text/javascript" src="slider/slider.js"></script>
    <!-- end of slider scripts -->

    <!-- scroll bar scripts -->
    <link rel="stylesheet" href="custom-scroll/website.css" type="text/css" media="screen"/>

    <!--<script type="text/javascript" src="custom-scroll/jquery-1.4.2.min.js"></script>-->
    <script type="text/javascript" src="custom-scroll/jquery.tinyscrollbar.min.js"></script>
    <script type="text/javascript">
      $(document).ready(function(){
        $('#scrollbar1').tinyscrollbar();
      });
    </script>
    <!-- end of scroll bar scripts -->
  </head>

  <body>
    <div class="mainContainer">
      <div class="userEditHolder">
        <div class="ueHeader">
          <img src="images/ue-nav-logo.jpg" />
          <img src="images/ue-nav-btn-help.jpg" />
          <img src="images/ue-nav-btn-my-account.jpg" />
          <img src="images/ue-nav-btn-logout.jpg" />
        </div>
        <div class="clear"></div>

        <div class="ueBody">
          <div class="ueNavHolder">
            <img src="images/ue-nav-bg-left.png" style="float:left;" />
            <div class="ueNavMid">
              <ul class="ueNav">
                <li><a href="#"><span>Website List</span></a></li>
                <li><a href="#"><span>System Stats</span></a></li>
                <li><a href="#"><span>Reports</span></a></li>
                <li><a href="#"><span>Ad Setup</span></a></li>
                <li><a href="#"><span>Content Library</span></a></li>
                <li class="select"><a href="#"><span>Users</span></a></li>
                <li><a href="#"><span>System Config</span></a></li>
              </ul>
            </div>
            <img src="images/ue-nav-bg-right.png" style="float:left;" />
          </div>

          <div class="ueBodyContent">
            <div style="float:left; color:#999999;">Here is where some brief instructional text could be placed for each page</div>
            <div class="btnBlueLeft" style="width:115px;">
              <%= form_for(@upload, :html => { :multipart => true }) do |f| %>
                <div class="field">
                  <%= f.file_field :upload %>
                </div>
                <div class="actions">
                  <%= f.submit "Create" %>
                </div>
              <% end %>

            </div>

            <!-- col 1 -->

            <div style="float:left; width:225px; margin:10px 0px;">
              <img src="images/seperator-dot.gif" style="margin:0px 5px;" />
              <img src="images/seperator-dot.gif" style="margin:0px 5px;" />
              <img src="images/seperator-dot.gif" style="margin:0px 5px;" />
            </div>
            <!-- end of rc table -->

          
            <!-- end of rc slider -->

            <div style="float:left; width:225px; margin:10px 0px;">
              <img src="images/seperator-dot.gif" style="margin:0px 5px;" />
              <img src="images/seperator-dot.gif" style="margin:0px 5px;" />
              <img src="images/seperator-dot.gif" style="margin:0px 5px;" />
            </div>

            <!-- options -->

            <!-- end of options -->

          </div>
          <!-- end of col1 -->

          <!-- col 2 -->

          <!-- grid body -->

          <!-- end of grid body -->

          <!-- end of col 2 -->

        </div>

        <div class="rcFooter" style="margin-left:28px;"></div>
        <div class="clear"></div>

        <div class="footerLinks" style="margin-left:28px;">

          <img src="images/icon-home.jpg" style="float:left; border:none; margin:3px 0px 0px 10px;" />

          <ul class="ueNavBottom">
            <li><a href="#"><span>Link 1</span></a></li>
            <li><a href="#"><span>Sub link 2</span></a></li>
            <li><a href="#"><span>link 3</span></a></li>
            <li><a href="#"><span>Sub link 4</span></a></li>
          </ul>
        </div>
      </div>
    </div>

    <script src="tabs/activatables.js" type="text/javascript"></script>
    <script type="text/javascript">
      activatables('page', ['page-1', 'page-2']);
    </script>
  </body>
</html>