<div class="ueHeader">
  <img src="/assets/users/images/ue-nav-logo.jpg"> <img src="/assets/users/images/ue-nav-btn-help.jpg"> <img src="/assets/users/images/ue-nav-btn-my-account.jpg"> <img src="/assets/users/images/ue-nav-btn-logout.jpg">
</div>
<div class="clear"></div>
<div class="ueBody">
  <div class="ueNavHolder">
    <img src="/assets/users/images/ue-nav-bg-left.png" style="float:left;">
    <div class="ueNavMid">
      <ul class="ueNav">
        <li>
          <a href="#"><span>Website List</span></a>
        </li>
        <li>
          <a href="#"><span>System Stats</span></a>
        </li>
        <li>
          <a href="#"><span>Reports</span></a>
        </li>
        <li>
          <a href="#"><span>Ad Setup</span></a>
        </li>
        <li >
          <a href="#"><span>Content Library</span></a>
        </li>
        <li class="select">
          <a href="#"><span>Users</span></a>
        </li>
        <li>
          <a href="#"><span>System Config</span></a>
        </li>
      </ul>
    </div><img src="/assets/users/images/ue-nav-bg-right.png" style="float:left;">
  </div>
  <div class="ueBodyContent">
    <!-- add buttons -->
    <div class="addButtons">
      <span class="addFile"><a href="#"></a></span> <span class="addForm"><a href="#"></a></span> <span class="addText"><a href="#"></a></span> <span class="addVideo"><a href="#"></a></span> <span class="addImage"><a href="#"></a></span>
    </div><!-- end of add buttons -->
    <div class="clear"></div><!-- col 1 -->
    <div class="userSearch">
      <!-- rc table -->
      <div class="rcPaleBlue" style="padding:50px 5px 10px 5px; position:relative;">
        <!-- search and select -->
        <input type="text" class="userSearchField"> <img src="/assets/users/images/icon-user-search-field.png" style="position:absolute; right:0; top:0;">
        <div class="selectDiv">
          <select class="select" title="Select one">
            <option>
              All columns
            </option>
            <option>
              Blue
            </option>
            <option selected="selected">
              Red
            </option>
            <option>
              Green
            </option>
            <option>
              Yellow
            </option>
            <option>
              Brown
            </option>
          </select>
        </div><!-- end of search and select -->
        <!-- names -->
        <div class="rcBlue2">
          <div class="rcBlack2">
            <span>Smith</span> <img src="/assets/users/images/icon-remove.png">
          </div><span>name</span>
        </div>
        <div class="rcBlue2">
          <div class="rcBlack2">
            <span>Admin</span> <img src="/assets/users/images/icon-remove-red.png">
          </div><span>role</span>
        </div><!-- end of names -->
      </div>
      <div style="float:left; width:225px; margin:10px 0px;">
        <img src="/assets/users/images/seperator-dot.gif" style="margin:0px 5px;"> <img src="/assets/users/images/seperator-dot.gif" style="margin:0px 5px;"> <img src="/assets/users/images/seperator-dot.gif" style="margin:0px 5px;">
      </div><!-- end of rc table -->
      <!-- slider rc -->
      <div class="rcPaleBlue" style="padding:5px 5px 5px 5px; position:relative;">
        <div class="searchOptions">
          <img src="/assets/users/images/icon-check-green.png"><span>date added</span>
        </div>
        <div class="clear"></div><input type="text" class="datePicker"><img src="/assets/users/images/icon-calender.png" class="calender"> <input type="text" class="datePickerRight"><img src="/assets/users/images/icon-calender.png" class="calender">
        <div class="clearHeight10"></div>
        <div class="searchOptions">
          <img src="/assets/users/images/icon-close-inactive.png"><span>date last used</span>
        </div>
      </div><!-- end of rc slider -->
      <div style="float:left; width:225px; margin:10px 0px;">
        <img src="/assets/users/images/seperator-dot.gif" style="margin:0px 5px;"> <img src="/assets/users/images/seperator-dot.gif" style="margin:0px 5px;"> <img src="/assets/users/images/seperator-dot.gif" style="margin:0px 5px;">
      </div><!-- slider rc -->
      <div class="rcPaleBlue" style="padding:5px 5px 5px 5px; position:relative;">
        <div class="searchOptions">
          <img src="/assets/users/images/icon-check-green.png"><span>times used</span>
        </div>
        <div id="slider" style="width:200px; margin-left:5px;"></div>
        <div class="clearHeight30"></div>
      </div><!-- end of rc slider -->
    </div><!-- end of col1 -->
    <!-- col 2 -->
    <div class="userGrid">
      <div class="userGridHeader">
        <ul class="ConLibraHeads">
          <li>
            <span class="name">Name<br><br><br></span>
            <span class="type">Type<br><br></span>
            <span class="name">Date Added<br><br></span>
            <span class="addedBy">Added By<br><br></span>
          </li>
        </ul>
      </div><!-- grid body -->
      <div class="userGridBody">
        <div id="scrollbar1">
          <div class="scrollbar">
            <div class="track">
              <div class="thumb">
                <div class="end"></div>
              </div>
            </div>
          </div>
          <div class="viewport">
            <div class="overview">
              <ul class="ConLibra">
                <%@users.each do |u|%>
                  <li class="dark">
                    <p>
                      <span class="name"><%=u.username%></span>
                    </p>
                    <span class="type"><%=u.roles.first.name%></span> 
                    <span class="name"><%=u.created_at.strftime("%m/%d/%Y %T")%></span>
                    <span class="addedBy">John</span> 
                    <span class="edit"><img src="/assets/users/images/btn-edit.png"></span>
                  </li>
                <%end%>
              </ul>
            </div>
          </div>
        </div>
      </div>
    </div><!-- end of grid body -->
    <div class="userGridFooter"></div>
    <div class="clear"></div>
  </div><!-- end of col 2 -->
</div>